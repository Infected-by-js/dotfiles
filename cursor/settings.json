{
    "vsicons.presets.hideExplorerArrows": true,
    "catppuccin-icons.hidesExplorerArrows": true,
    "apc.menubar.compact": true,
    "apc.header": {
        "height": 30
    },
    "apc.sidebar.titlebar": {
        "height": 30
    },
    "apc.statusBar": {
        "position": "editor-bottom",
        "height": 30
    },
    "apc.listRow": {
        "lists": [
            "customview-tree",
            "results",
            "open-editors",
            "explorer-folders-view",
            "outline-tree",
            "quick-input-list",
            "tree"
        ],
        "height": 24,
        "fontSize": 19
    },
    "apc.imports": [
        "file://~/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.3/dist/updateHandler.js"
    ],
    "apc.electron": {
        "frame": false,
        "transparent": true,
        "titleBarStyle": "hiddenInset",
        "vibrancy": "hud",
        "opacity": 0.95,
        "visualEffectState": "active"
    },
    "apc.font.family": "Lekton Nerd Font",
    "apc.monospace.font.family": "Iosevka",
    "editor.fontFamily": "Maple Mono, SF Mono, Monoid Nerd Font, RecMonoLinear Nerd Font, ZedMono Nerd Font, Lekton Nerd Font Mono, ProFontWindows Nerd Font Mono, Iosevka, IosevkaTermSlab Nerd Font, Victor Mono, Geist Mono, SauceCodePro Nerd Font Mono, Fragment Mono, MesloLGS Nerd Font, FiraCode Nerd Font, JetBrainsMono Nerd Font Mono",
    "terminal.integrated.fontFamily": "MesloLGS Nerd Font",
    "terminal.integrated.fontSize": 16,
    "editor.fontSize": 15,
    "editor.lineHeight": 1.5,
    "editor.fontWeight": 200,
    "apc.activityBar": {
        "position": "bottom",
        "size": 30,
        "itemSize": 30
    },
    "apc.stylesheet": {
        ".sidebar .title-label": "padding: 0 !important;",
        ".quick-input-widget.show-file-icons, .editor-widget.find-widget": "box-shadow: none !important;",
        ".quick-input-widget": "transform: translateY(-50%) !important; top: 50% !important; box-shadow: 0px 8px 20px rgba(0, 0, 0, .45) !important; backdrop-filter: blur(3px) !important; border-radius: 8px !important;"
    },
    "animations.Install-Method": "Apc Customize UI++",
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "git-graph.dialog.addTag.type": "Lightweight",
    "git-graph.graph.style": "angular",
    "git-graph.graph.uncommittedChanges": "Open Circle at the Checked Out Commit",
    "git-graph.referenceLabels.alignment": "Branches (aligned to the graph) & Tags (on the right)",
    "git-graph.repository.commits.fetchAvatars": true,
    "git.openRepositoryInParentFolders": "never",
    "javascript.preferences.renameMatchingJsxTags": false,
    "typescript.preferences.renameMatchingJsxTags": false,
    "window.commandCenter": false,
    "editor.formatOnSave": false,
    "prettier.requireConfig": true,
    "prettier.useEditorConfig": false,
    "files.autoSaveWhenNoErrors": true,
    "editor.accessibilitySupport": "off",
    "extensions.ignoreRecommendations": true,
    "cSpell.userWords": [
        "abortable",
        "aggr",
        "clsx",
        "composables",
        "endregion",
        "filedata",
        "Finres",
        "fullscreen",
        "maxt",
        "Nuxt",
        "pinia",
        "unnessasery",
        "unsub",
        "Vaul",
        "vueuse",
        "Вопросы"
    ],
    "terminal.explorerKind": "both",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "supermaven.enableFixWithSupermaven": true,
    "workbench.iconTheme": "catppuccin-latte",
    "workbench.colorTheme": "Bearded Theme Black & Ruby",
    "vim.leader": "<space>",
    "vim.useSystemClipboard": false,
    "editor.lineNumbers": "relative",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.foldfix": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<esc>"
            ]
        },
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<esc>"
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                "editor.action.triggerSuggest"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "w"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "b"
            ],
            "commands": [
                "workbench.action.showAllEditors"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "r"
            ],
            "commands": [
                "workbench.action.showAllEditorsByMostRecentlyUsed"
            ]
        },
        {
            "before": [
                "<S-h>"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "<S-l>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            "before": [
                "<S-Tab>"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            "before": [
                "leader",
                "|"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "leader",
                "-"
            ],
            "commands": [
                ":split"
            ]
        },
        {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "commands": [
                "workbench.action.focusBelowGroup"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                ":w!"
            ]
        },
        {
            "before": [
                "leader",
                "q"
            ],
            "commands": [
                ":q!"
            ]
        },
        {
            "before": [
                "Q"
            ],
            "commands": [
                "workbench.action.closeAllEditors"
            ]
        },
        {
            "before": [
                "leader",
                "x"
            ],
            "commands": [
                ":x!"
            ]
        },
        {
            "before": [
                "[",
                "d"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "]",
                "d"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "i"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "a"
            ],
            "commands": [
                "editor.action.codeAction"
            ]
        },
        {
            "before": [
                "leader",
                "s",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "leader",
                "p"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "leader",
                "o"
            ],
            "commands": [
                "editor.action.organizeImports"
            ]
        },
        {
            "before": [
                "leader",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.explorer.fileView.focus",
                "workbench.files.action.showActiveFileInExplorer"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "J"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1,
        "asvetliakov.vscode-neovim": 1
    },
    "editor.suggest.insertMode": "insert",
    "editor.glyphMargin": false,
    "editor.minimap.enabled": false,
    "explorer.compactFolders": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "symbols.hidesExplorerArrows": true,
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.tips.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.showFoldingControls": "never",
    "editor.overviewRulerBorder": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontLigatures": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "workbench.tree.enableStickyScroll": false,
    "workbench.startupEditor": "none",
    "zenMode.hideLineNumbers": false,
    "breadcrumbs.symbolPath": "off",
    "editor.codeLens": false,
    "editor.linkedEditing": true,
    "workbench.editor.enablePreview": false,
    "editor.renderLineHighlight": "none",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "gitlens.ai.experimental.model": "openai:gpt-4o",
    "update.mode": "none",
    "animations.Enabled": true,
    "screencastMode.verticalOffset": 30,
    "editor.cursorSurroundingLines": 10,
    "database-client.autoSync": true,
    "cSpell.language": "en,ru",
    "vscodeCustomCodeColor.fileLanguageIdList": [
        "vue",
        "typescript"
    ],
    "vscodeCustomCodeColor.highlightValue": "v-role",
    "vscodeCustomCodeColor.highlightValueColor": "#6366f1",
    "extensions.autoCheckUpdates": false,
    "update.showReleaseNotes": false,

    "custom-ui-style.electron": {
        "workbench.activityBar.location": "bottom",
        "titleBarStyle": "hiddenInset",
        "trafficLightPosition": {
          "x": 20,
          "y": 16
        }
      },
      "custom-ui-style.stylesheet": {
